<p id="notice"><%= notice %></p>
<% if user_signed_in? %>

<h1>Bugs</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Deadline</th>
      <th>Screenshot</th>
      <th>Type</th>
      <th>Status</th>
      
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bugs.each do |bug| %>
      <tr>
        <td><%= bug.title %></td>
        <td><%= bug.deadline %></td>
        <td><%= bug.screenshot %></td>
        <td><%= bug.type %></td>
        <td><%= bug.status %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%= link_to "Submit Link", new_bug_path %></td>
        
     </td>
        </tr>
    <% end %>
  </tbody>
</table>


<%end%>

